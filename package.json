{
  "name": "tankila",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "cannon-es": "^0.18.0",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "mongoose": "^5.11.11",
    "three": "^0.132.2",
    "unzipper": "^0.10.11",
    "ws": "^7.4.6"
  },
  "scripts": {
    "client-dev": "webpack serve --config ./client/webpack.dev.ts",
    "client-prod": "webpack --config ./client/webpack.prod.ts && ts-node -T client/tools/copy-resources && ts-node -T client/tools/unzip",
    "prestart": "npm run build-client",
    "server-dev": "cd server && nodemon --watch src/** --ext ts,json --exec ts-node ./src/index",
    "server-prod": "cd server && ts-node -T ./src/index.ts",
    "start": "npm run client-prod && npm run server-prod",
    "lint": "eslint ."
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/copy-webpack-plugin": "^8.0.1",
    "@types/dat.gui": "^0.7.7",
    "@types/express": "^4.17.13",
    "@types/fs-extra": "^9.0.13",
    "@types/mini-css-extract-plugin": "^2.4.0",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^14.14.14",
    "@types/three": "^0.132.1",
    "@types/unzipper": "^0.10.4",
    "@types/webpack": "^5.28.0",
    "@types/webpack-dev-server": "^4.1.0",
    "@types/ws": "^8.2.0",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "copy-webpack-plugin": "^9.0.1",
    "css-loader": "^6.4.0",
    "eslint": "^7.16.0",
    "fs-extra": "^10.0.0",
    "html-loader": "^2.1.2",
    "html-webpack-plugin": "^5.3.2",
    "mini-css-extract-plugin": "^2.4.3",
    "node-sass": "^4.14.1",
    "nodemon": "^2.0.14",
    "sass-loader": "^12.2.0",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.3",
